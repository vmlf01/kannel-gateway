	{
		/* Abort(SUSPEND) all method and push transactions */
		wsp_abort_methods(sm, WSP_ABORT_SUSPEND);

		/* S-Suspend.ind(abort reason) */
		wsp_indicate_suspend(sm, e->abort_code);
	},
	SUSPENDED)

