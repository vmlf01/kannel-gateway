		wsp_send_abort(WSP_ABORT_SUSPEND, sm->resume_handle);

		/* Abort(SUSPEND) all method transactions */
		wsp_abort_methods(sm, WSP_ABORT_SUSPEND);

		/* S-Suspend.ind(SUSPEND) */
