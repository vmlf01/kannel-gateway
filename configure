     echo "$ac_t""checking malloc" 1>&6
     ;;
  slow) cat >> confdefs.h <<\EOF
#define USE_GWMEM_SLOW 1
EOF

        echo "$ac_t""slow malloc" 1>&6
	;;
  *) cat >> confdefs.h <<\EOF
#define USE_GWMEM_NATIVE 1
EOF

          echo "$ac_t""native malloc" 1>&6
          ;;
  esac

